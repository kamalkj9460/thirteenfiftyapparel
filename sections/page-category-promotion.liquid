{%- if section.blocks.size > 0 -%}
{%- capture section_class -%}
{% if section.settings.has_container %}container p-0{% endif %}
{%- endcapture -%}
{%- assign splide_custom_css = '--splide-arrow-size:2em;--splide-arrow-size-md:3em;--splide-arrow-bgc:#c6c6c66e;--splide-arrow-br:0;' -%}
<div class="section-outer" style="background-color:{{section.settings.color_bg}};{% if section.settings.pattern != 0 %}{% render 'ssh-pattern', _pattern: section.settings.pattern, _fillcolor: section.settings.color_pattern %}{% endif %}">
    <div id="creative-builder-{{section.id}}" class="{{section_class}}" data-section-id="{{ section.id }}" data-section-type="creative-builder" style="{{splide_custom_css}}">
        {%- if section.settings.title != empty and section.settings.heading_hide == false -%}
            <div class="cb-head col-p-100 pb-3 pt-4 d-flex align-items-start{% if section.settings.heading_center %} justify-content-center{% else %} justify-content-center justify-content-md-start{% endif %}{% if section.settings.has_padding %} px-1{% endif %}" style="--border-color:{{section.settings.underline_color}};--font-size-lg:{{section.settings.heading_fontsize}}px;--font-size-md:{{section.settings.heading_fontsize | times: 0.78 }}px;--font-size:25px">
                <span class="h3 custom-font-size{% if section.settings.heading_underline %} border-bottom pb-1{% if section.settings.underline_color != blank and section.settings.underline_color != 'rgba(0,0,0,0)' %} custom-border-color{% endif %}{% endif %}">{{section.settings.title}}</span>
                {%- if section.settings.heading_button != blank -%}
                <a class="btn-modern ml-3" href="{{section.settings.heading_button_url}}">{{section.settings.heading_button}}</a>
                {%- endif -%}
            </div>
        {%- endif -%}
        <div class="cb-body col-p-100{% if section.settings.has_padding_smallscreen %} px-3{% endif %}{% if section.settings.has_padding %} py-sm-1{% if section.settings.has_container %} px-sm-1{% endif %}{% else %} px-sm-0{% endif %}">
            <div class="cb-body-wrapper creative-builder{% if section.settings.hori_scroll_mobile == true %} rt-mobile-snapscroll{% endif %}{% if section.settings.center_content %} justify-content-md-center{% endif %}">        
                {%- assign group_mode = false -%}
                {%- assign slider_mode = false -%}
                {%- assign tab_mode = false -%}
                {%- assign tab_underline = false -%}
                {%- assign special_types = 'group-start,group-end,tab-start,tab-end' | split: ',' -%}

                {%- for block in section.blocks -%}
                {% capture block_class %}cb-item{% if block.settings.has_padding %} p-sm-1{% endif %}{% if slider_mode == true %} splide__slide{% else %} col-p{% endif %}{% endcapture %}
                {% capture block_pcol_css %}--pcol:{{block.settings.width}}%;--pmcol:{{block.settings.width_mobile}}%;{% endcapture %}
                {% capture block_id %}cb-item-{{block.id}}{% endcapture %}
                {% capture border_class %}
                {% if block.settings.border_top %} border-top-md{% endif %}{% if block.settings.border_bottom %} border-bottom-md{% endif %}{% if block.settings.border_left %} border-left-md{% endif %}{% if block.settings.border_right %} border-right-md{% endif %}{% if block.settings.mobile_border_top %} border-top-xs-only{% endif %}{% if block.settings.mobile_border_bottom %} border-bottom-xs-only{% endif %}{% if block.settings.mobile_border_left %} border-left-xs-only{% endif %}{% if block.settings.mobile_border_right %} border-right-xs-only{% endif %}{% endcapture %}
            
                {%- unless special_types contains block.type -%}
                {%- if tab_mode == true -%}
                <div class="rt-tab">
                    <input class="rt-tab__radio" type="radio" name="rt-tabgroup-{{section.id}}" id="rt-tab-{{section.id}}{{block.id}}"{% if tab_first_item == true %} checked{% endif %}>
                    {%- if block.settings.title != empty -%}
                    <label class="h3{% if tab_underline == true %} pb-1 border-bottom{% if tab_underline_color != blank and tab_underline_color != 'rgba(0,0,0,0)' %} custom-border-color{% endif %}{% endif %}" for="rt-tab-{{section.id}}{{block.id}}">{{block.settings.title}}</label>
                    {%- endif -%}
                    <div class="rt-tab__content d-flex" {% if block.settings.height %} data-rt-tab--height="{{block.settings.height}}"{% endif %}>
                {%- assign tab_first_item = false -%}        
                {%- endif -%}
                {%- endunless -%}

                {%- case block.type -%}
                {%- when 'banner' -%}
                
                {%- assign color_bg = block.settings.color_bg -%}

                <div id="{{block_id}}" class="position-relative {{block_class}}{{border_class}} text-pos-left custom-min-height" style="{{block_pcol_css}}--min-height:{{block.settings.mobile_height}}px;--min-height-md:{{block.settings.height}}px" {{ block.shopify_attributes }}>
                    {%- if block.settings.cta_text != blank or block.settings.linklist_enabled == true -%}
                    <div class="banner-content-wrapper text-vpos-top{% if block.settings.bg_image != blank %} bgimage-center-no-repeat lazyload{% endif %}" style="background-color:{{color_bg}};{% if block.settings.pattern > 0 %}{% render 'ssh-pattern', _pattern: block.settings.pattern, _opacity: '0.25', _fillcolor: block.settings.color_pattern %}{% endif %}"{% if block.settings.bg_image != blank %}{% assign img_url = block.settings.bg_image | img_url: "master" %} data-bg="{{img_url}}"{% endif %}>
                    {%-else-%}
                    <a href="{{block.settings.linked_url}}" class="banner-content-wrapper text-vpos-top{% if block.settings.bg_image != blank %} bgimage-center-no-repeat lazyload{% endif %}" style="background-color:{{color_bg}};;{% if block.settings.pattern > 0 %}{% render 'ssh-pattern', _pattern: block.settings.pattern, _opacity: '0.25', _fillcolor: block.settings.color_pattern %}{% endif %}"{% if block.settings.bg_image != blank %}{% assign img_url = block.settings.bg_image | img_url: "master" %} data-bg="{{img_url}}"{% endif %}>
                    {%-endif-%}
                        <div class="banner-texting p-2 position-absolute custom-absolute-pos{% if block.settings.content_align_hori %} text-align-center{% else %} {{block.settings.text_align}}{% endif %} without--fx{% if block.settings.cta == blank %} without--cta{% endif %}{% if block.settings.linklist_enabled == false %} without--linklist{% endif %}{% if block.settings.content_align_hori %} flex-nowrap flex-row{% endif %}" style="--top-side:{{block.settings.side_padding_vert}}%;--left-side:{{block.settings.side_padding_hori}}%;transform: translate({% if block.settings.side_padding_hori > 20 %}-50%{% else %}0{% endif %},{% if block.settings.side_padding_vert > 20 %}-50%{% else %}0{% endif %});">
                            {%-if block.settings.heading != blank-%}
                            <div class="b-heading h3{% if block.settings.heading_shadow == true %} has--text-shadow{% endif %}{% if block.settings.heading_bgcolor_use %} p-2 custom-bgcolor{% else %}{% if block.settings.content_align_hori == true %} mr-3{% else %} mb-3{% endif %}{% endif %} custom-font-size custom-font-weight custom-color" style="--bg-color:{{block.settings.heading_bgcolor}};--font-weight:{{block.settings.heading_weight}};--font-size-lg:{{block.settings.heading_size}}px;--font-size-md:{{block.settings.heading_size | times: 0.7 }}px;--font-size:22px;--color:{{block.settings.heading_color}};">
                            {{block.settings.heading}}
                            </div>
                            {%-endif-%}
                
                            {%-if block.settings.cap != blank-%}
                            <div class="b-caption {%if block.settings.cap_shadow == true %} has--text-shadow{%-endif-%}{% if block.settings.cap_bgcolor_use %} p-2 custom-bgcolor{% endif %}{% if block.settings.content_align_hori == true %} mr-4{% else %} mb-4{% endif %} custom-font-size custom-color custom-font-weight" style="--bg-color:{{block.settings.cap_bgcolor}};--font-weight:{{block.settings.cap_weight}};--font-size-lg:{{block.settings.cap_size}}px;--font-size-md:{{block.settings.cap_size | times: 0.7 }}px;--font-size:18px;--color:{{block.settings.cap_color}};">
                            {{block.settings.cap}}
                            </div>	
                            {%-endif-%}
                            
                            {%- if block.settings.linklist_enabled == true -%}
                            {%- if block.settings.linklist != blank -%}
                            {%-assign _linklist = linklists[block.settings.linklist] -%}
                            <ul class="pl-0 custom-color " style="column-count:{{block.settings.linklist_col}};--color:{{block.settings.linklist_color}};--font-size:{{block.settings.linklist_size}}px;--font-weight:{{block.settings.linklist_weight}}">
                            {%- for _link in _linklist.links -%}
                                <li class="d-table pr-2 pr-md-5 pb-1" >
                                    <a href="{{_link.url}}" class="custom-font-size custom-font-weight">{{_link.title}}</a>
                                </li>
                            {%- endfor -%}
                            </ul>
                            {%- else -%}
                            <ul class="pl-0 custom-color " style="column-count:{{block.settings.linklist_col}};--color:{{block.settings.linklist_color}};--font-size:{{block.settings.linklist_size}}px;--font-weight:{{block.settings.linklist_weight}}">
                                {%- for i in (1..12) -%}
                                    <li class="d-table pr-2 pr-md-5 pb-1" >
                                        <a href="#" class="custom-font-size custom-font-weight">MENU ITEM</a>
                                    </li>
                                {%- endfor -%}
                            </ul>
                            {%-endif-%}
                            {%- endif -%}
                
                            {%-if block.settings.cta_text != blank-%}
                            <a class="{% if block.settings.modern %}btn-modern{% else %}btn-squared custom-bgcolor{% endif %} cta custom-font-size custom-font-weight custom-color" href="{{block.settings.linked_url}}" style="--font-weight:{{block.settings.cta_weight}};--font-size:18px;--font-size-md:{{block.settings.cta_size}}px;--color:{{block.settings.cta_color}};{% if block.settings.modern == false %}--bg-color:{{block.settings.cta_bgcolor}}{% endif %}">{{block.settings.cta_text}}</a>
                            {%-endif-%}
                        </div>
                    {%-if block.settings.cta_text == blank and block.settings.linklist_enabled == false-%}
                    </a>{%-else-%}
                    </div>{%-endif-%}
                </div>
                        
                {%- when 'promotion' -%}
                {%- assign color_bg = block.settings.color_bg -%}
                
                {%- if block.settings.icon_pos == 'column' or block.settings.icon_pos == 'column-reverse' -%}
                {%- assign promo_class = 'position-absolute h-100 w-100 d-flex flex-' | append: block.settings.icon_pos -%}
                {%- assign promo_class = promo_class | append: ' align-items-' | append: block.settings.hori_alignment -%}
                {%- assign promo_class = promo_class | append: ' justify-content-' | append: block.settings.vert_alignment -%}
                {%- else -%}
                {%- assign promo_class = 'position-absolute h-100 w-100 d-flex flex-column flex-md-' | append: block.settings.icon_pos -%}
                {%- assign promo_class = promo_class | append: ' align-items-center align-items-md-' | append: block.settings.vert_alignment -%}
                {%- assign promo_class = promo_class | append: ' justify-content-center justify-content-md-' | append: block.settings.hori_alignment -%}
                {%- endif -%}

                {%- capture var_style -%}
                --font-size:{{block.settings.font_size}}px;
                --svg-size:{{block.settings.icon_size}}px;
                --font-weight:{{block.settings.font_weight}};
                {%-if block.settings.color_text != empty and block.settings.color_text != 'rgba(0,0,0,0)' -%}--color:{{block.settings.color_text}};--svg-color:{{block.settings.color_text}};{%- endif -%}
                --bg-color:{{color_bg}};
                --min-height:{{block.settings.mobile_height}}px;--min-height-md:{{block.settings.height}}px
                {%- endcapture -%}
                <div id="cb-item-{{block.id}}" class="{{block_class}}{{border_class}} custom-min-height" style="{{block_pcol_css}}{{var_style}}" {{ block.shopify_attributes }}>  
                    {% if block.settings.linked_url != empty %}
                    <a class="w-100 custom-bgcolor position-relative" href="{{block.settings.linked_url}}">
                    {% else %}
                    <div class="w-100 custom-bgcolor position-relative">
                    {% endif %}    
                        <div class="{{promo_class}} custom-absolute-pos" style="--top-side:{{block.settings.side_padding_vert}}%;--left-side:{{block.settings.side_padding_hori}}%">
                            <div class="p-1 lh--1 custom-svg-size custom-svg-color"><svg><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#global__symbols--{{block.settings.icon}}"></use></svg></div>
                            <div class="p-1 d-flex align-items-center" ><p class="m-0 custom-font-size custom-font-weight custom-color text-center">{{block.settings.title}}</p></div>
                        </div>
                    {% if block.settings.linked_url == empty %}
                    </div>
                    {% else %}
                    </a>
                    {% endif %}
                </div>

                {%- when 'collection' -%}
                
                <div style="{{block_pcol_css}}--snapscroll-itemsize:250px" id="cb-item-{{block.id}}" class="{{block_class}} collection{% if block.settings.is_slider %} is-slider no-pagination splide splide--arrowpos--{{block.settings.slider_arrow_pos}}{% else %} flex-wrap{% if block.settings.hori_scroll_mobile == true %} rt-mobile-snapscroll{% endif %}{% endif %}{% if section.settings.has_container %} has-container{% endif %}"{% if block.settings.is_slider %} data-splide='{"type":"slide","perMove":"1","perPage":"{{block.settings.slider_items}}","breakpoints":{"767":{% if block.settings.hori_scroll_mobile == true %}{"destroy":"true"}}{% else %}{"perPage":"{{block.settings.slider_items_mobile}}"}}{% endif %}}'{% endif %} {{ block.shopify_attributes }}>
                    {% if block.settings.is_slider %}
                    {%- if section.settings.has_container -%}
                    <span class="position-absolute h-100 zi-lv9 ml-n-container custom-bgcolor w-container d-none d-sm-block" style="--bg-color:{{settings.color_body_bg}};"></span>
                    <span class="absolute-right h-100 zi-lv9 mr-n-container custom-bgcolor w-container d-none d-sm-block" style="--bg-color:{{settings.color_body_bg}};"></span>
                    {%- endif -%}
                    {%- if block.settings.hori_scroll_mobile == true -%}
                    {%- render 'slider-nav-buttons', custom_class: 'd-none d-md-block' -%}
                    {%- else -%}
                    {%- render 'slider-nav-buttons' -%}
                    {%- endif -%}
                    <div class="splide__track w-100">
                        <div class="splide__list w-100{% if block.settings.hori_scroll_mobile == true %} rt-mobile-snapscroll{% endif %}">
                    {% endif %}
                        {%- if block.settings.collection_id != empty -%}
                        {%- assign _collection = collections[block.settings.collection_id] -%}
                        {%- for product in _collection.products limit: block.settings.limit -%}
                            <div class="{% if block.settings.is_slider %}splide__slide p-sm-1{% else %}col-6 col-md-4 col-lg-3 p-1{% endif %}">
                            {%- include 'product-item' -%}
                            </div>
                        {%- endfor -%}
                        {%- else -%}
                        {%- for i in (1..block.settings.limit) -%}
                            <div class="{% if block.settings.is_slider %}splide__slide p-sm-1{% else %}col-6 col-md-4 col-lg-3 p-1{% endif %}">
                                <div class="d-flex flex-column align-items-center p-2" style="--bg-color:#efefef">
                                    {{ 'product-1' | placeholder_svg_tag: 'custom-bgcolor' }}
                                    <span class="h4 pt-2">PRODUCT ITEM</span>
                                </div>
                            </div>
                        {%- endfor -%}
                        {%- endif %}
                    {% if block.settings.is_slider %}
                        </div>
                    </div>
                    {% endif %}
                </div>

                {%- when 'product' -%}
                <div id="cb-item-{{block.id}}" class="{{block_class}} product" style="{{block_pcol_css}}" {{ block.shopify_attributes }}>
                {%- if block.settings.product_id != empty -%}
                {%- assign product = all_products[block.settings.product_id] -%}
                {%- include 'product-item', custom_badge: block.settings.product_badge -%}
                {%- else -%}
                <div class="d-flex flex-column align-items-center p-2" style="--bg-color:#efefef">
                    {{ 'product-1' | placeholder_svg_tag: 'custom-bgcolor' }}
                    <span class="h4 pt-2">PRODUCT ITEM</span>
                </div>
                {%- endif %}
                </div>

                {%- when 'video' -%}
                <div id="cb-item-{{block.id}}" class="{{block_class}} video position-relative" style="{{block_pcol_css}}" {{ block.shopify_attributes }}>
                    {%- if block.settings.video_source == 'youtube' -%}
                <div class="yt-video--cover{% if block.settings.cover_img != blank %} bgimage-center-no-repeat lazyload{% endif %} position-absolute w-100 h-100 zi-lv1 d-flex custom-svg-size justify-content-center align-items-center" style="--svg-size:40px;--svg-size-md: 100px;--color:#fff;{% if block.settings.cover_img == blank %}background-color:#808080;{% render 'ssh-pattern', _pattern: 2 %}{% endif %}"{% if block.settings.cover_img != blank %} data-bg="{{block.settings.cover_img | img_url: 'master'}}"{% endif %}>
                        <div class="position-absolute custom-absolute-pos self-center-y {% if section.settings.has_container == false and block.settings.width == 100 %} ml-container pl-3 pl-sm-0{% else %} ml-5{% endif %}" style="--custom-gutter:0px;--top-side:50%">
                            {%-if block.settings.title != blank-%}
                            <div class="h3 mb-1 mb-lg-3 custom-font-size custom-font-weight custom-color" style="--font-weight:{{block.settings.heading_weight}};--font-size-lg:{{block.settings.heading_size}}px;--font-size-md:{{block.settings.heading_size | times: 0.7 }}px;--font-size:20px;--color:{{block.settings.heading_color}};">
                            {{block.settings.title}}
                            </div>
                            {%-endif-%}
                            {%-if block.settings.title != blank-%}
                            <div class="b-caption custom-font-size custom-color custom-font-weight" style="--font-weight:{{block.settings.cap_weight}};--font-size-lg:{{block.settings.cap_size}}px;--font-size-md:{{block.settings.cap_size | times: 0.7 }}px;--font-size:14px;--color:{{block.settings.cap_color}};">
                            {{block.settings.cap}}
                            </div>	
                            {%-endif-%}
                        </div>
                        <svg class="has-pointer yt-video--play-bt custom-color">
                            <use xlink:href="#global__symbols--solid--play"></use>
                        </svg>
                    </div>
                    {%- endif -%}
                    <div class="w-100" style="--aspect-ratio: {{block.settings.ratio}};" data-product-single-media-wrapper data-media-id="cb-item-{{block.id}}" >
                        {%- if block.settings.video_source == 'youtube' -%}
                        <iframe class="w-100" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" 
                            allowfullscreen="allowfullscreen" 
                            src="https://www.youtube.com/embed/{{block.settings.youtube_url.id}}?controls=1&amp;enablejsapi=1&amp;modestbranding=1&amp;playsinline=1&amp;rel=0" 
                            title="">
                        </iframe>
                        {%- else -%}
                        <video playsinline="playsinline" class="media-video" preload="metadata" poster="{%- if block.settings.cover_img != blank -%}{{block.settings.cover_img | img_url: 'master'}}{%- endif -%}">
                            <source src="{{block.settings.mp4_url}}" type="video/mp4">
                        </video>
                        {%- endif -%}
                    </div>
                </div>

                {%- when 'image' -%}
                <div id="cb-item-{{block.id}}" class="{{block_class}}{{border_class}} image" style="{{block_pcol_css}}">
                    {%- assign _class = 'w-100 d-flex flex-' | append: block.settings.image_pos | append: ' ' | append: block.settings.text_align -%}
                    {%- if block.settings.fadeoff -%}
                    {%- assign _class = _class | append: ' p-4 align-items-center of-hidden position-relative' -%}
                    {%- endif -%}
                    {%-if block.settings.linked_url != blank-%}
                    <a href="{{block.settings.linked_url}}" class="{{_class}}" {%if block.settings.text == blank%} style="line-height:0;" {%endif%}>
                    {%-else-%}
                    <div class="{{_class}}">
                        {%-endif-%}
                        {%-if block.settings.image_pos != blank-%}
                        {%-if block.settings.image == blank-%}
                        <div class="cb-item--image">
                            <div class="d-flex flex-column align-items-center p-1" style="--bg-color:#efefef">
                                {{ 'image' | placeholder_svg_tag: 'custom-bgcolor' }}
                            </div>
                        </div>
                        {%-else-%}
                        {%- assign img_url = block.settings.image | img_url: 'master' -%}
                        <div class="{% if block.settings.fadeoff %} fade-off-bg{% endif %}">
                        <img class="lazyload w-100" src="data:image/svg+xml,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 5 1'%3E%3C/svg%3E" data-src="{{img_url}}" alt="{{block.settings.text | strip_html}}"/>
                        </div>
                        {%-endif-%}
                        {%-endif-%}
                        {%- if block.settings.text != empty -%}
                        <div class="cb-item--head{% if block.settings.image_pos == 'row' or block.settings.image_pos == 'row-reverse' %} px-2{% endif %}{% if block.settings.fadeoff %} absolute-bottom pb-2{% endif %}" style="font-weight:{{block.settings.text_weight}};font-size:{{block.settings.text_size}}px;color:{{block.settings.text_color}};">
                        {{block.settings.text}}
                        </div>
                        {%-endif-%}
                        {%-if block.settings.linked_url == blank-%}
                    </div>
                    {%-else-%}
                    </a>
                    {%-endif-%}
                </div>

                {%- when 'review' -%}
                <div id="cb-item-{{block.id}}" class="{{block_class}} review" style="{{block_pcol_css}}">
                    <div class="d-flex flex-column w-100 p-3 align-items-center" style="background-color:{{block.settings.color_bg}}">
                        <div class="" style="--height:0.75rem">
                            <svg class="icon custom-height" >
                                <use xlink:href="#stars-{{block.settings.star}}-0-star">
                            </svg>
                        </div>
                        <div class="no-margin--p py-2{% if block.settings.align_center %} text-center{% endif %}" style="--font-size:{{block.settings.fontsize_comment}}px;--color:{{block.settings.color_comment}}"><p class="custom-font-size custom-color">{{block.settings.comment}}</p></div>
                        <div class="custom-font-size custom-color" style="--font-size:{{block.settings.fontsize_signature}}px;--color:{{block.settings.color_signature}}">{{block.settings.signature}}</div>
                    </div>
                </div>

                {%- when 'article' -%}
                <div id="cb-item-{{block.id}}" class="{{block_class}} article flex-column" style="{{block_pcol_css}}background-color:{{block.settings.color_bg}}">
                    <div class="d-flex flex-column w-100">
                    {%- assign _article = articles[block.settings.article_id] -%}
                    {%- if _article != empty -%}
                    {%- if _article.image != empty -%}
                        <a href="{{_article.url}}" class="cb-article--media w-100 lazyload bgimage-center-no-repeat" data-bg="{{_article.image.src | img_url: 'master'}}" style="--aspect-ratio:{{block.settings.ratio}}"></a>
                    {%- endif -%}
                        <a class="py-2 custom-font-size custom-color text-uppercase" href="{{_article.url}}" style="--font-size:{{block.settings.fontsize_title}}px;--color:{{block.settings.color_title}}">{{_article.title}}</a>
                        <div class="pb-1 custom-font-size custom-color text-uppercase font-weight-normal" style="--font-size:{{block.settings.fontsize_info}}px;--color:{{block.settings.color_title}}"><span>{{_article.author}}</span> {{'blogs.general.on' | t}} <span>{{ _article.created_at | date: "%b %d, %Y" }}</span></div>
                        <div class="custom-font-size custom-color" style="--font-size:{{block.settings.fontsize_info}}px;--color:{{block.settings.color_commentcount}}">{{'blogs.comments.comments_with_count' | t: count: _article.comments_count}}</div>
                    {%- else -%}
                        <div class="d-flex flex-column align-items-center p-1" style="--bg-color:#efefef">
                            {{ 'image' | placeholder_svg_tag: 'custom-bgcolor w-100' }}
                        </div>
                        <a class="py-2 custom-font-size custom-color text-uppercase" href="#" style="--font-size:{{block.settings.fontsize_title}}px;--color:#000">BLOG POST TITLE</a>
                        <div class="pb-1 custom-font-size custom-color text-uppercase font-weight-normal" style="--font-size:20px;--color:{{block.settings.color_title}}"><span>AUTHOR</span> {{'blogs.general.on' | t}} <span>AUG 01, 2020</span></div>
