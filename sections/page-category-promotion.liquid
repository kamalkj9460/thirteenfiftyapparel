                    {%- assign _items = linklists[block.settings.links].links -%}
                    <div class="d-flex flex-column w-100">
                        <div class="pb-3 custom-font-size custom-color custom-font-weight" style="--color:{{block.settings.color_title}};--font-size:{{block.settings.fontsize_title}}px;--font-weight:{{block.settings.fontweight_title}}">{%- if block.settings.title != blank -%}{{block.settings.title}}{%- else -%}{{linklists[block.settings.links].title}}{%- endif -%}</div>
                        <div>
                            <ul class="m-0 pl-0 pr-lg-5" style="column-count:{{block.settings.col}};--color:{{block.settings.color_item}};--font-size:{{block.settings.fontsize_item}}px;--font-weight:{{block.settings.fontweight_item}}">
                                {%- for _item in _items -%}
                                <li class="d-table pr-2 custom-font-size custom-color custom-font-weight"><a href="{{_item.url}}">{{_item.title}}</a></li>
                                {%- endfor -%}
                            </ul>
                        </div>
                    </div>
                    {%- endif -%}
