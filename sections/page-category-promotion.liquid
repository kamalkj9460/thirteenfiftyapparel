                {%- when 'tab-end' -%}
                {%- if tab_mode == true -%}
                {%- assign tab_mode = false -%}
                {%- assign tab_underline = false -%}
                    </div>
                </div>
                {%- endif -%}

                {%- endcase -%}

                {%- unless special_types contains block.type -%}
                {%- if tab_mode == true -%}
                </div></div>
                {%- endif -%}
                {%- endunless -%}
                
                {%- endfor -%}
            </div>
        </div>
    </div>
</div>
{%- endif -%}
