{% schema %}

{
    "name": "Creative Builder",
    "class": "index-section section-wrapper",
    "settings": [
        {
            "type": "header",
            "content": "Heading"
        },
        {
            "type": "text",
            "id": "title",
            "label": "Heading"
        },
        {
            "type": "checkbox",
            "id": "heading_hide",
            "label": "Hide heading",
            "default": false
        },
        {
            "type": "checkbox",
            "id": "heading_underline",
            "label": "Underline heading",
            "default": false
        },
        {
            "type": "color",
            "id": "underline_color",
            "label": "Color",
            "default": "#3a3a3a",
            "info": "Select None to use the default color"
